CREATE TABLE Public."bi2" ("Paper_ID" Varchar(100), "Included" Varchar(10), "Observation_ID" Varchar(100), "Author" Varchar(50), "Year" numeric, "Title" Varchar(1000), "Journal" Varchar(100), "DOI" Varchar(2048), "URL" Varchar(2048), "Language" Varchar(100), "Database" Varchar(100), "Latitude" numeric, "Longitude" numeric, "Country" Varchar(100), "Taxa_level" Varchar(100), "Taxa_name" Varchar(100),"Orders" Varchar(100), "Family" Varchar(100), "Genus" Varchar(100), "Binomial" Varchar(100), "Life_history_stage" Varchar(100), "Experimental_year_start" numeric, "Experimental_year_end" numeric, "Sampling_method" Varchar(100),"Biodiversity_metric" Varchar(100), "Unit" Varchar(100), "IUCN_threat_category_1" Varchar(100), "IUCN_threat_category_2" Varchar(100), "IUCN_threat_category_3" Varchar(100), "Treatment" Varchar(100), "Treatment_quantity" Varchar(100), "Treatment_quantity_unit" Varchar(100), "Control" Varchar(100), "Extracted_from" Varchar(100), "Evidence_type" Varchar(100), "Treatment_N" numeric, "Treatment_mean" numeric, "Treatment_error" numeric, "Treatment_error_type" Varchar(100), "Control_N" numeric, "Control_mean" numeric, "Control_error" numeric, "Control_error_type" Varchar(100), "Contributor_name" Varchar(100), "Search_date" Varchar(10),"Notes" Varchar(100));

